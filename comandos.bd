----------------------------  COMANDOS CRIAÇÃO DO BANCO DE DADOS   ----------------------------

CREATE DATABASE projeto_pi;

CREATE TABLE admin {
    id int auto_increment primary key,
    login varchar(100),
    senha varchar(100)
};

CREATE TABLE usuario {
    id int auto_increment primary key,
    
    nome_completo varchar(250),

    CPF varchar(13),

    CEP varchar(11),

    num_telefone varchar(15),

    email varchar(100),

    senha(250)
};





